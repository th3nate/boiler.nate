/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@app/*": ["src/app/*"],
      "@core/*": ["src/app/@core/*"],
      "@shared/*": ["src/app/@shared/*"],
      "@env/*": ["src/environments/*"]
    },
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
